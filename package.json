{
  "name": "wink-nlp",
  "version": "0.0.0",
  "description": "Natural Language Processing",
  "keywords": [
    "NLP",
    "wink"
  ],
  "main": "src/wink-nlp.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc --reporter=html --reporter=text mocha ./test/",
