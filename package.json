    "lint": "eslint ./src/*.js ./test/*.js ./runkit/*.js",
    "bench": "node benchmark/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winkjs/wink-nlp.git"
  },
  "author": "Sanjaya Kumar Saxena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/winkjs/wink-nlp/issues"
  },
  "homepage": "http://winkjs.org/",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "docker": "^1.0.0",
    "eslint": "^7.0.0",
    "nyc": "^15.0.1",
    "jsdoc": "^3.6.4",
    "mocha": "^7.1.2"
  },
  "runkitExampleFilename": "./runkit/example.js",
  "dependencies": {}
